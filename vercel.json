{
  "builds": [
    {
      "src": "mi-frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "mi-backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/mi-backend/index.js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/mi-frontend/dist/index.html" }
  ]
}